    "gatsby-plugin-offline": "^4.4.0",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sharp": "^3.4.0",
    "gatsby-plugin-styled-components": "^4.4.0",
    "gatsby-plugin-typescript": "^3.4.0",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-source-graphql": "^3.4.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-plugin-styled-components": "^1.12.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4",
    "typescript-styled-plugin": "^0.16.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "yarn run start",
    "lint": "eslint src/**/*.{js,ts,tsx} *.js",
    "lint:fix": "yarn run lint --fix",
    "start": "concurrently \"gatsby develop\" \"yarn run codegen\"",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "codegen": "graphql-codegen --config graphql-codegen.yml"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "graphql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spawnia/gatsby-starter-typescript-graphql"
  }
