{
  "name": "gatsby-starter-typescript-graphql",
  "description": "A Gatsby starter with typesafe GraphQL using TypeScript",
  "version": "0.2.0",
  "author": {
    "name": "Benedikt Franke",
    "email": "benedikt@franke.tech",
    "url": "https://franke.tech"
  },
  "dependencies": {
    "gatsby": "^3.4.0",
    "gatsby-image": "^3.4.0",
